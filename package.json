{
  "name": "admate",
  "PascalCasedName": "Admate",
  "version": "0.10.0",
  "private": false,
  "description": "Admin CRUD frontend framework for Vue 2.6/2.7/3, extremely concise without losing flexibility.",
  "author": {
    "name": "Cloyd Lau",
    "url": "https://github.com/cloydlau"
  },
  "license": "MIT",
  "repository": "https://github.com/cloydlau/admate",
  "keywords": [
    "admin",
    "admin-interfaces",
    "assistant",
    "helper",
    "manage",
    "management",
    "vue-pure-admin",
    "vue-vben-admin",
    "vue-element-admin",
    "vue-demi",
    "axios",
    "antd",
    "ant-design",
    "ant-design-vue",
    "element",
    "element-ui",
    "element-plus",
    "prime-vue",
    "quasar",
    "vuetify"
  ],
  "exports": {
    ".": {
      "types": "./dist/admate.d.ts",
      "import": "./dist/admate.mjs",
      "require": "./dist/admate.umd.js"
    },
    "./*": "./*"
  },
  "main": "./dist/admate.umd.js",
  "module": "./dist/admate.mjs",
  "unpkg": "./dist/admate.umd.js",
  "jsdelivr": "./dist/admate.umd.js",
  "types": "./dist/admate.d.ts",
  "files": [
    "dist",
    "docs"
  ],
  "scripts": {
    "dev": "esno ./scripts/dev.mts",
    "doc": "vitepress dev --open /README",
    "build": "vite build",
    "serve": "vite preview",
    "release": "esno ./scripts/release.mts",
    "lint": "eslint \"**/*.{vue,js,ts,mjs,mts,jsx,tsx,md,json}\"",
    "lint:fix": "eslint \"**/*.{vue,js,ts,mjs,mts,jsx,tsx,md,json}\" --fix",
    "preinstall": "node -e \"if (process.env.INIT_CWD === process.cwd()) { process.exit(1) }\" || npx only-allow pnpm",
    "postinstall": "node -e \"if (process.env.INIT_CWD === process.cwd()) { process.exit(1) }\" || npx simple-git-hooks"
  },
  "peerDependencies": {
    "@vue/composition-api": ">=1",
    "vue": "2||3"
  },
  "peerDependenciesMeta": {
    "@vue/composition-api": {
      "optional": true
    }
  },
  "dependencies": {
    "vue-demi": "^0.14.7"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.12.1",
    "@commitlint/cli": "^19.2.1",
    "@commitlint/config-conventional": "^19.1.0",
    "@element-plus/icons-vue": "^2.3.1",
    "@mdi/font": "^7.4.47",
    "@quasar/vite-plugin": "^1.6.0",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.12.4",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vue/compiler-sfc": "^3.4.21",
    "@vue/test-utils": "^2.4.5",
    "admate": "workspace:*",
    "ant-design-vue": "^4.1.2",
    "axios": "^1.6.8",
    "case-police": "^0.6.1",
    "cross-spawn": "^7.0.3",
    "del": "^7.1.0",
    "element-plus": "^2.6.3",
    "eslint": "npm:eslint-ts-patch@8.57.0-0",
    "eslint-plugin-format": "^0.1.0",
    "eslint-ts-patch": "8.57.0-0",
    "esno": "^4.7.0",
    "faim": "^0.6.14",
    "kolorist": "^1.8.0",
    "lint-staged": "^15.2.2",
    "lodash-es": "^4.17.21",
    "magicast": "^0.3.3",
    "mockjs": "^1.1.0",
    "only-allow": "^1.2.1",
    "open": "^10.1.0",
    "primeicons": "^7.0.0",
    "primevue": "^3.51.0",
    "prompts": "^2.4.2",
    "qs": "^6.12.0",
    "quasar": "^2.15.2",
    "rollup-plugin-visualizer": "^5.12.0",
    "sass": "^1.74.1",
    "semver": "^7.6.0",
    "simple-git-hooks": "^2.11.1",
    "typescript": "^5.4.4",
    "unocss": "0.59.0-beta.1",
    "unplugin-auto-import": "^0.17.5",
    "unplugin-vue-components": "^0.26.0",
    "vite": "^5.2.8",
    "vite-plugin-dts": "^3.8.1",
    "vite-plugin-mock": "^3.0.1",
    "vitepress": "^1.0.2",
    "vue": "^3.4.21",
    "vue-global-config": "^0.6.0",
    "vue-router": "^4.3.0",
    "vuetify": "^3.5.14",
    "zhlint": "^0.8.1"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged",
    "commit-msg": "npx --no -- commitlint --edit ${1}"
  },
  "lint-staged": {
    "*.md": [
      "case-police --fix",
      "zhlint --fix"
    ],
    "*.{vue,js,ts,mjs,mts,jsx,tsx,md,json}": "eslint --ignore-pattern demo/* --cache --fix"
  }
}
