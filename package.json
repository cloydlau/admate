{
  "name": "admate",
  "PascalCasedName": "Admate",
  "version": "0.9.8",
  "private": false,
  "description": "Assistant/Helper of admin interfaces for Vue 2.6/2.7/3, extremely concise without losing flexibility.",
  "author": {
    "name": "Cloyd Lau",
    "url": "https://github.com/cloydlau"
  },
  "license": "MIT",
  "repository": "https://github.com/cloydlau/admate",
  "keywords": [
    "admin",
    "admin-interfaces",
    "manage",
    "management",
    "vue-pure-admin",
    "vue-vben-admin",
    "vue-element-admin",
    "crud",
    "vue-demi",
    "axios",
    "antd",
    "ant-design",
    "ant-design-vue",
    "element",
    "element-ui",
    "element-plus",
    "prime-vue",
    "quasar",
    "vuetify"
  ],
  "exports": {
    ".": {
      "types": "./dist/admate.d.ts",
      "import": "./dist/admate.mjs",
      "require": "./dist/admate.umd.js"
    },
    "./*": "./*"
  },
  "main": "./dist/admate.umd.js",
  "module": "./dist/admate.mjs",
  "unpkg": "./dist/admate.umd.js",
  "jsdelivr": "./dist/admate.umd.js",
  "types": "./dist/admate.d.ts",
  "files": [
    "dist",
    "docs"
  ],
  "scripts": {
    "dev": "esno ./scripts/dev.mts",
    "up": "nu -L !@antfu/eslint-config",
    "doc": "vitepress dev --open /README",
    "build": "vite build",
    "serve": "vite preview",
    "release": "esno ./scripts/release.mts",
    "pnpm:devPreinstall": "node ./scripts/devPreinstall.js",
    "preinstall": "node -e \"if (process.env.INIT_CWD === process.cwd()) { process.exit(1) }\" || npx only-allow pnpm",
    "postinstall": "node -e \"if (process.env.INIT_CWD === process.cwd()) { process.exit(1) }\" || npx simple-git-hooks"
  },
  "peerDependencies": {
    "@vue/composition-api": ">=1",
    "vue": "2||3"
  },
  "peerDependenciesMeta": {
    "@vue/composition-api": {
      "optional": true
    }
  },
  "dependencies": {
    "vue-demi": "^0.14.6"
  },
  "devDependencies": {
    "@antfu/eslint-config": "~0.42.1",
    "@commitlint/cli": "latest",
    "@commitlint/config-conventional": "latest",
    "@element-plus/icons-vue": "latest",
    "@mdi/font": "latest",
    "@quasar/vite-plugin": "latest",
    "@types/lodash-es": "latest",
    "@types/node": "latest",
    "@vitejs/plugin-vue": "latest",
    "@vue/compiler-sfc": "latest",
    "@vue/test-utils": "latest",
    "admate": "workspace:*",
    "ant-design-vue": "latest",
    "axios": "latest",
    "case-police": "latest",
    "cross-spawn": "latest",
    "del": "latest",
    "element-plus": "latest",
    "esno": "latest",
    "faim": "0.4.0-alpha.7",
    "kolorist": "latest",
    "lint-staged": "latest",
    "lodash-es": "latest",
    "magicast": "latest",
    "mockjs": "latest",
    "only-allow": "latest",
    "open": "latest",
    "primeicons": "latest",
    "primevue": "latest",
    "prompts": "latest",
    "qs": "latest",
    "quasar": "latest",
    "sass": "latest",
    "semver": "latest",
    "simple-git-hooks": "latest",
    "typescript": "latest",
    "unocss": "latest",
    "unplugin-auto-import": "latest",
    "unplugin-vue-components": "latest",
    "vite": "latest",
    "vite-plugin-dts": "latest",
    "vite-plugin-mock": "latest",
    "vitepress": "1.0.0-rc.24",
    "vue": "latest",
    "vue-global-config": "latest",
    "vue-router": "latest",
    "vuetify": "latest",
    "zhlint": "latest"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm exec lint-staged --concurrent false",
    "commit-msg": "npx --no -- commitlint --edit ${1}"
  },
  "lint-staged": {
    "*.md": [
      "case-police --fix",
      "zhlint --fix"
    ],
    "{scripts,src}": "eslint --cache --fix"
  }
}