{
  "name": "admate",
  "PascalCasedName": "Admate",
  "version": "0.9.5",
  "private": false,
  "packageManager": "pnpm@7.18.2",
  "description": "Mate / Helper / Assistant of admin interfaces for Vue 2.6 / 2.7 / 3, extremely concise without losing flexibility.",
  "author": "cloydlau",
  "license": "MIT",
  "repository": "https://github.com/cloydlau/admate",
  "keywords": [
    "crud",
    "admin",
    "manage",
    "management",
    "admin-interfaces",
    "vue",
    "vue-demi",
    "vue-vben-admin",
    "vue-element-admin",
    "axios",
    "element",
    "element-ui",
    "element-plus",
    "quasar",
    "vuetify",
    "prime-vue",
    "ant-design-vue",
    "antd"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/admate.umd.js",
      "import": "./dist/admate.mjs"
    },
    "./*": "./*"
  },
  "main": "./dist/admate.umd.js",
  "module": "./dist/admate.mjs",
  "unpkg": "./dist/admate.umd.js",
  "jsdelivr": "./dist/admate.umd.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "docs"
  ],
  "scripts": {
    "dev3": "cl switchVue 3 && vite --config ./demo/vue3/vite.config.ts --open",
    "dev2.7": "cl switchVue 2.7 && vite --config ./demo/vue2.7/vite.config.ts --open",
    "dev2.6": "cl switchVue 2.6 && vite --config ./demo/vue2.6/vite.config.ts --open",
    "serve": "vite preview",
    "build": "vite build",
    "doc": "vitepress dev --open /README",
    "release": "npx case-police --fix && npx lint-staged && pnpm build && cl release"
  },
  "peerDependencies": {
    "@vue/composition-api": ">=1",
    "vue": "2 || 3"
  },
  "peerDependenciesMeta": {
    "@vue/composition-api": {
      "optional": true
    }
  },
  "dependencies": {
    "lodash-es": "^4.17.21",
    "vue-demi": "^0.13.11",
    "vue-global-config": "^0.3.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.34.0",
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@mdi/font": "^7.1.96",
    "@quasar/vite-plugin": "^1.2.3",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^18.11.15",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/compiler-sfc": "^3.2.45",
    "@vue/test-utils": "^2.2.6",
    "admate": "^0.9.4",
    "ant-design-vue": "^3.2.15",
    "axios": "^1.2.1",
    "case-police": "^0.5.10",
    "element-plus": "^2.2.26",
    "eslint": "^8.29.0",
    "husky": "^8.0.2",
    "lint-staged": "^13.1.0",
    "mockjs": "^1.1.0",
    "primeicons": "^6.0.1",
    "primevue": "^3.21.0",
    "qs": "^6.11.0",
    "quasar": "^2.10.2",
    "sass": "^1.56.2",
    "typescript": "^4.9.4",
    "unocss": "^0.47.6",
    "unplugin-auto-import": "^0.12.1",
    "vite": "^4.0.1",
    "vite-plugin-dts": "^1.7.1",
    "vite-plugin-mock": "^2.9.6",
    "vitepress": "^1.0.0-alpha.32",
    "vue": "^3.2.45",
    "vue-router": "^4.1.6",
    "vuetify": "^3.0.5"
  },
  "lint-staged": {
    "{src,__tests__}/**.*": [
      "eslint --fix"
    ]
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  }
}