{
  "name": "admate",
  "description": "Mate of admin interfaces",
  "author": "cloydlau",
  "version": "0.7.16",
  "private": false,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/cloydlau/admate"
  },
  "main": "./dist/admate.umd.js",
  "module": "./dist/admate.es.js",
  "exports": {
    ".": {
      "import": "./dist/admate.es.js",
      "require": "./dist/admate.umd.js"
    }
  },
  "scripts": {
    "dev:2": "cd ./demo/vue2 && npm run dev",
    "dev:3": "cd ./demo/vue3 && npm run dev",
    "build": "vite build",
    "serve": "vite preview",
    "cov": "jest --ci --coverage",
    "jest": "jest --watchAll",
    "c": "rd /s /q .\\node_modules",
    "ri": "rd /s /q .\\node_modules && pnpm i",
    "test:2": "vue-demi-switch 2 && jest",
    "test:3": "vue-demi-switch 3 vue3 && jest",
    "u": "node scripts/upgrade.js",
    "release": "node scripts/release.js"
  },
  "dependencies": {
    "kayran": "latest",
    "lodash-es": "latest",
    "vue-demi": "latest"
  },
  "peerDependencies": {
    "@vue/composition-api": "1",
    "vue": "^2.0.0 || >=3.0.0"
  },
  "peerDependenciesMeta": {
    "@vue/composition-api": {
      "optional": true
    }
  },
  "devDependencies": {
    "@vue/composition-api": "latest",
    "chalk": "^4.1.2",
    "enquirer": "latest",
    "execa": "^4.1.0",
    "minimist": "latest",
    "rollup-plugin-peer-deps-external": "latest",
    "rollup-plugin-visualizer": "latest",
    "semver": "latest",
    "tslib": "latest",
    "typescript": "latest",
    "vite": "latest",
    "vitest": "latest"
  },
  "files": [
    "dist"
  ],
  "jest": {
    "coverageDirectory": "./coverage/",
    "collectCoverage": true
  }
}
