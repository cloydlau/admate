{
  "name": "admate",
  "PascalCasedName": "Admate",
  "type": "commonjs",
  "version": "1.0.0-rc.0",
  "private": false,
  "description": "Admin CRUD frontend meta framework for Vue 2/3, extremely concise without losing flexibility.",
  "author": {
    "name": "Cloyd Lau",
    "url": "https://github.com/cloydlau"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cloydlau/admate.git"
  },
  "keywords": [
    "admin",
    "admin-interfaces",
    "assistant",
    "helper",
    "manage",
    "management",
    "vue-pure-admin",
    "vue-vben-admin",
    "vue-element-admin",
    "vue-demi",
    "axios",
    "antd",
    "ant-design",
    "ant-design-vue",
    "element",
    "element-ui",
    "element-plus",
    "prime-vue",
    "quasar",
    "vuetify"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/admate.d.mts",
        "default": "./dist/admate.mjs"
      },
      "require": {
        "types": "./dist/admate.d.ts",
        "default": "./dist/admate.umd.js"
      }
    },
    "./*": "./*"
  },
  "main": "./dist/admate.umd.js",
  "module": "./dist/admate.mjs",
  "unpkg": "./dist/admate.umd.js",
  "jsdelivr": "./dist/admate.umd.js",
  "types": "./dist/admate.d.ts",
  "files": [
    "dist",
    "docs"
  ],
  "scripts": {
    "dev": "esno ./scripts/dev.mts",
    "typegen": "npx tsup --entry.admate src/index.js --format esm,cjs --cjsInterop --clean --dts-only",
    "build": "vite build && pnpm typegen",
    "doc": "vitepress dev --open /RsEADME",
    "serve": "vite preview",
    "release": "esno ./scripts/release.mts",
    "sync-to-cnpm": "npx cnpm sync && curl -L https://npmmirror.com/sync/admate",
    "lint": "eslint \"**/*.{js,mjs,cjs,jsx,ts,mts,cts,tsx,vue,json,md,html,css,scss,sass}\"",
    "lint:fix": "eslint \"**/*.{js,mjs,cjs,jsx,ts,mts,cts,tsx,vue,json,md,html,css,scss,sass}\" --fix",
    "preinstall": "node -e \"if (process.env.INIT_CWD === process.cwd()) { process.exit(1) }\" || (npx only-allow pnpm && pnpm config set registry https://registry.npmmirror.com)",
    "postinstall": "node -e \"if (process.env.INIT_CWD === process.cwd()) { process.exit(1) }\" || (npx simple-git-hooks && git config core.hooksPath .git/hooks/ && npx license-checker --summary --out ./licenses.txt)",
    "lint:inspect": "npx @eslint/config-inspector@latest"
  },
  "peerDependencies": {
    "@vue/composition-api": ">=1",
    "vue": "2||3"
  },
  "peerDependenciesMeta": {
    "@vue/composition-api": {
      "optional": true
    }
  },
  "dependencies": {
    "lodash-es": "^4.17.21",
    "vue-demi": "^0.14.10",
    "vue-global-config": "^0.6.3"
  },
  "devDependencies": {
    "@antfu/eslint-config": "latest",
    "@arethetypeswrong/cli": "^0.18.2",
    "@commitlint/cli": "latest",
    "@commitlint/config-conventional": "latest",
    "@element-plus/icons-vue": "latest",
    "@eslint/eslintrc": "latest",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^24.2.0",
    "@unocss/eslint-config": "latest",
    "@vitejs/plugin-vue": "latest",
    "@vue/compiler-sfc": "latest",
    "@vue/test-utils": "latest",
    "admate": "workspace:*",
    "axios": "^1.11.0",
    "browserslist": "latest",
    "case-police": "latest",
    "cnpm": "^9.4.0",
    "cross-spawn": "^7.0.6",
    "del": "^8.0.0",
    "element-plus": "latest",
    "eslint": "latest",
    "eslint-plugin-financial": "latest",
    "eslint-plugin-format": "latest",
    "eslint-plugin-no-unsanitized": "latest",
    "esno": "^4.8.0",
    "faim": "^0.12.3",
    "kolorist": "^1.8.0",
    "license-checker": "latest",
    "lint-staged": "latest",
    "magicast": "^0.3.5",
    "mockjs": "^1.1.0",
    "npm-deprecated-check": "latest",
    "only-allow": "latest",
    "open": "^10.2.0",
    "prompts": "^2.4.2",
    "publint": "^0.3.12",
    "qs": "^6.14.0",
    "rollup-plugin-visualizer": "^6.0.3",
    "sass": "^1.89.2",
    "semver": "^7.7.2",
    "simple-git-hooks": "latest",
    "tsup": "^8.5.0",
    "typescript": "^5.9.2",
    "unocss": "~66.4.1",
    "unplugin-auto-import": "^20.0.0",
    "unplugin-unused": "^0.5.1",
    "unplugin-vue-components": "^28.8.0",
    "vite": "^7.0.6",
    "vite-plugin-mock": "^3.0.2",
    "vitepress": "^1.6.3",
    "vue": "latest",
    "vue-router": "latest",
    "zhlint": "latest"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged",
    "commit-msg": "npx --no -- commitlint --edit ${1}"
  },
  "lint-staged": {
    "*.md": [
      "case-police --fix",
      "zhlint --fix"
    ],
    "*.{js,mjs,cjs,jsx,ts,mts,cts,tsx,vue,json,md,html,css,scss,sass}": "eslint --cache --fix"
  },
  "browserslist": [
    "last 6 years",
    "last 2 major versions",
    "not dead",
    "> 0.2%",
    "chrome >= 87",
    "firefox >= 78",
    "firefox esr",
    "safari >= 14",
    "edge >= 88",
    "ios >= 12",
    "android >= 9"
  ]
}
