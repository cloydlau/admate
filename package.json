{
  "name": "admate",
  "PascalCasedName": "Admate",
  "version": "0.9.5",
  "private": false,
  "packageManager": "pnpm@7.18.2",
  "description": "Mate/Helper/Assistant of admin interfaces for Vue 2.6/2.7/3, extremely concise without losing flexibility.",
  "author": "cloydlau",
  "license": "MIT",
  "repository": "https://github.com/cloydlau/admate",
  "keywords": [
    "crud",
    "admin",
    "manage",
    "management",
    "back-office",
    "admin-interfaces",
    "vue",
    "vue-demi",
    "vue-vben-admin",
    "vue-element-admin",
    "axios",
    "element",
    "element-ui",
    "element-plus",
    "quasar",
    "vuetify",
    "prime-vue",
    "ant-design-vue",
    "antd"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/admate.umd.js",
      "import": "./dist/admate.mjs"
    },
    "./*": "./*"
  },
  "main": "./dist/admate.umd.js",
  "module": "./dist/admate.mjs",
  "unpkg": "./dist/admate.umd.js",
  "jsdelivr": "./dist/admate.umd.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "docs"
  ],
  "scripts": {
    "dev3": "cl switchVue 3 && vite --config ./demo/vue3/vite.config.ts --open",
    "dev2.7": "cl switchVue 2.7 && vite --config ./demo/vue2.7/vite.config.ts --open",
    "dev2.6": "cl switchVue 2.6 && vite --config ./demo/vue2.6/vite.config.ts --open",
    "build": "vite build",
    "doc": "vitepress dev --open /README",
    "release": "npx lint-staged && pnpm build && cl release"
  },
  "peerDependencies": {
    "@vue/composition-api": ">=1",
    "vue": "2||3"
  },
  "peerDependenciesMeta": {
    "@vue/composition-api": {
      "optional": true
    }
  },
  "dependencies": {
    "lodash-es": "latest",
    "vue-demi": "latest",
    "vue-global-config": "latest"
  },
  "devDependencies": {
    "@commitlint/cli": "latest",
    "@commitlint/config-conventional": "latest",
    "@mdi/font": "latest",
    "@quasar/vite-plugin": "latest",
    "@types/lodash-es": "latest",
    "@types/node": "latest",
    "@vitejs/plugin-vue": "latest",
    "@vue/compiler-sfc": "latest",
    "@vue/test-utils": "latest",
    "admate": "latest",
    "ant-design-vue": "latest",
    "axios": "latest",
    "case-police": "latest",
    "element-plus": "latest",
    "husky": "latest",
    "lint-staged": "latest",
    "mockjs": "latest",
    "primeicons": "latest",
    "primevue": "latest",
    "qs": "latest",
    "quasar": "latest",
    "rome": "latest",
    "sass": "latest",
    "typescript": "latest",
    "unocss": "latest",
    "unplugin-auto-import": "latest",
    "vite": "latest",
    "vite-plugin-dts": "latest",
    "vite-plugin-mock": "latest",
    "vitepress": "latest",
    "vue": "latest",
    "vue-router": "latest",
    "vuetify": "latest",
    "zhlint": "latest"
  },
  "lint-staged": {
    "*.md": [
      "case-police --fix",
      "zhlint --fix"
    ]
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  }
}
