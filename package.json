    "vue2",
    "vue3",
    "vben",
    "vue-vben-admin",
    "vue-element-admin"
  ],
  "exports": {
    ".": {
      "require": "./dist/admate.umd.js",
      "import": "./dist/admate.mjs"
    }
  },
  "main": "./dist/admate.umd.js",
  "module": "./dist/admate.mjs",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "cl switchVue 3 && vite --config ./demo/vue3/vite.config.ts --open",
    "dev2.7": "cl switchVue 2.7 && vite --config ./demo/vue2.7/vite.config.ts --open",
    "dev2": "cl switchVue 2 && vite --config ./demo/vue2/vite.config.ts --open",
    "build": "vite build",
    "serve": "vite preview"
  },
  "peerDependencies": {
    "@vue/composition-api": "1"
