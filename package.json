    "ant-design-vue1": "npm:ant-design-vue@1",
    "axios": "latest",
    "element-plus": "latest",
    "element-ui": "latest",
    "eslint": "latest",
    "husky": "latest",
    "kikimore": "latest",
    "lint-staged": "latest",
    "mockjs": "latest",
    "primeicons": "latest",
    "primevue": "latest",
    "primevue2": "npm:primevue@2",
    "qs": "latest",
    "quasar": "latest",
    "sass": "latest",
    "semver": "latest",
    "typescript": "latest",
    "unocss": "latest",
    "vite": "latest",
    "vite-plugin-mock": "latest",
    "vitest": "latest",
    "vuetify": "next",
    "vuetify2": "npm:vuetify@2",
    "@vue/test-utils": "1",
    "vue": "2.7",
    "vue-router": "3"
  },
  "lint-staged": {
    "*.{js,ts,tsx,vue,md}": [
      "eslint --fix"
    ]
