{
  "name": "admate",
  "version": "0.8.1",
  "private": false,
  "description": "Mate of admin interfaces",
  "author": "cloydlau",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/cloydlau/admate"
  },
  "keywords": [
    "admin",
    "crud",
    "element-ui",
    "element-plus",
    "vuetify",
    "antdvue",
    "antd",
