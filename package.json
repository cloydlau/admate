{
  "name": "admate",
  "description": "Mate of admin interfaces",
  "author": "cloydlau",
  "version": "0.7.12",
  "private": false,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/cloydlau/admate"
  },
  "main": "./dist/admate.umd.js",
  "module": "./dist/admate.es.js",
  "exports": {
    ".": {
      "import": "./dist/admate.es.js",
      "require": "./dist/admate.umd.js"
    }
  },
  "scripts": {
    "dev": "vue-demi-switch 3 vue && vite",
    "dev:2": "vue-demi-switch 2 vue && vite --config vite.vue2.config.ts",
    "build": "vite build",
    "serve": "vite preview",
    "cov": "jest --ci --coverage",
    "jest": "jest --watchAll",
    "clean": "rd /s /q .\\node_modules",
    "ri": "rd /s /q .\\node_modules && pnpm i",
    "test:2": "vue-demi-switch 2 && jest",
    "test:3": "vue-demi-switch 3 vue3 && jest",
    "release": "node scripts/release.js"
  },
  "dependencies": {
    "kayran": "latest",
    "lodash-es": "latest",
    "vue-demi": "latest"
  },
  "peerDependencies": {
    "@vue/composition-api": "1",
    "axios": "0",
    "vue": "^2.0.0 || >=3.0.0"
  },
  "peerDependenciesMeta": {
    "@vue/composition-api": {
      "optional": true
    }
  },
  "devDependencies": {
    "@quasar/extras": "latest",
    "@quasar/vite-plugin": "latest",
    "@types/jest": "latest",
    "@vitejs/plugin-vue": "latest",
    "@vue/compiler-sfc": "latest",
    "@vue/composition-api": "latest",
    "ant-design-vue": "^2.2.8",
    "axios": "latest",
    "chalk": "^4.1.2",
    "element-plus": "latest",
    "enquirer": "latest",
    "execa": "^4.1.0",
    "jest": "latest",
    "minimist": "latest",
    "mitt": "latest",
    "mockjs": "latest",
    "primeicons": "latest",
    "primevue": "^3.10.0",
    "quasar": "latest",
    "rollup-plugin-peer-deps-external": "latest",
    "rollup-plugin-visualizer": "latest",
    "sass": "latest",
    "semver": "latest",
    "ts-jest": "latest",
    "tslib": "latest",
    "typescript": "latest",
    "unocss": "0.12.2",
    "vite": "latest",
    "vite-plugin-mock": "latest",
    "vue": "npm:vue@^3.2.21",
    "vue-router": "^4.0.12",
    "vuetify": "^3.0.0-alpha.12"
  },
  "files": [
    "dist"
  ],
  "jest": {
    "coverageDirectory": "./coverage/",
    "collectCoverage": true
  }
}
