{
  "name": "admate",
  "description": "Mate of admin interfaces",
  "author": "cloydlau",
  "version": "0.6.0-alpha.0",
  "private": false,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/cloydlau/admate"
  },
  "main": "./dist/admate.umd.js",
  "module": "./dist/admate.es.js",
  "exports": {
    ".": {
      "import": "./dist/admate.es.js",
      "require": "./dist/admate.umd.js"
    }
  },
  "scripts": {
    "dev": "vue-demi-switch 3 && vite",
    "dev:2": "vue-demi-switch 2 vue2 && vite",
    "build": "vite build",
    "serve": "vite preview",
    "cov": "jest --ci --coverage",
    "jest": "jest --watchAll",
    "clean": "rd /s /q .\\node_modules",
    "ri": "rd /s /q .\\node_modules && pnpm i",
    "test:2": "vue-demi-switch 2 vue2 && jest",
    "test:3": "vue-demi-switch 3 && jest"
  },
  "dependencies": {
    "kayran": "latest",
    "lodash-es": "latest",
    "qs": "latest",
    "vue-demi": "latest"
  },
  "peerDependencies": {
    "@vue/composition-api": "*",
    "axios": "*",
    "vue": "^2.0.0 || >=3.0.0"
  },
  "peerDependenciesMeta": {
    "@vue/composition-api": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/jest": "latest",
    "@types/overlayscrollbars": "latest",
    "@vitejs/plugin-vue": "latest",
    "@vue/compiler-sfc": "latest",
    "@vue/composition-api": "latest",
    "ant-design-vue": "^2.2.8",
    "ant-design-vue2": "npm:ant-design-vue@^1.7.8",
    "axios": "latest",
    "codecov": "latest",
    "element-plus": "latest",
    "jest": "latest",
    "mockjs": "latest",
    "overlayscrollbars": "latest",
    "rollup-plugin-peer-deps-external": "latest",
    "ts-jest": "latest",
    "tslib": "latest",
    "typescript": "latest",
    "vite": "latest",
    "vite-plugin-mock": "latest",
    "vite-plugin-windicss": "latest",
    "vue": "^3.2.19",
    "vue-router": "^4.0.11",
    "vue2": "npm:vue@^2.6.14",
    "vue2-router": "npm:vue-router@^3.5.2",
    "vuetify": "^3.0.0-alpha.11",
    "vuetify2": "npm:vuetify@^2.5.9",
    "windicss": "latest"
  },
  "files": [
    "dist"
  ],
  "jest": {
    "coverageDirectory": "./coverage/",
    "collectCoverage": true
  }
}
