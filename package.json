{
  "name": "admate",
  "version": "0.9.3",
  "private": false,
  "description": "Mate / Helper / Assistant of admin interfaces for Vue 2.6 / 2.7 / 3, extremely concise without losing flexibility.",
  "author": "cloydlau",
  "license": "MIT",
  "repository": "https://github.com/cloydlau/admate",
  "keywords": [
    "admin-interfaces",
    "crud",
    "element-ui",
    "element-plus",
    "vuetify",
    "antd",
    "ant-design-vue",
    "quasar",
    "prime-vue",
    "axios",
    "vue",
    "vue-vben-admin",
    "vue-element-admin",
    "vue-demi",
    "management",
    "manage"
  ],
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "require": "./dist/admate.umd.js",
      "import": "./dist/admate.mjs"
    },
    "./*": "./*"
  },
  "main": "./dist/admate.umd.js",
  "module": "./dist/admate.mjs",
  "unpkg": "./dist/admate.umd.js",
  "jsdelivr": "./dist/admate.umd.js",
  "types": "./dist/src/index.d.ts",
  "files": [
    "dist",
    "docs"
  ],
  "scripts": {
    "dev3": "cl switchVue 3 && vite --config ./demo/vue3/vite.config.ts --open",
    "dev2.7": "cl switchVue 2.7 && vite --config ./demo/vue2.7/vite.config.ts --open",
    "dev2.6": "cl switchVue 2.6 && vite --config ./demo/vue2.6/vite.config.ts --open",
    "serve": "vite preview",
    "build": "vite build",
    "doc": "vitepress dev --open /README",
    "release": "cl switchVue 3 && npx case-police --fix && npx lint-staged && pnpm build && cl release"
  },
  "peerDependencies": {
    "@vue/composition-api": ">=1",
    "vue": "2 || 3"
  },
  "peerDependenciesMeta": {
    "@vue/composition-api": {
      "optional": true
    }
  },
  "dependencies": {
    "lodash-es": "latest",
    "vue-demi": "latest",
    "vue-global-config": "latest"
  },
  "devDependencies": {
    "@antfu/eslint-config": "latest",
    "@commitlint/cli": "latest",
    "@commitlint/config-conventional": "latest",
    "@mdi/font": "latest",
    "@quasar/vite-plugin": "latest",
    "@vitejs/plugin-vue": "latest",
    "@vue/compiler-sfc": "latest",
    "@vue/test-utils": "latest",
    "admate": "latest",
    "ant-design-vue": "latest",
    "axios": "latest",
    "case-police": "latest",
    "element-plus": "latest",
    "eslint": "latest",
    "husky": "latest",
    "lint-staged": "latest",
    "mockjs": "latest",
    "primeicons": "latest",
    "primevue": "latest",
    "qs": "latest",
    "quasar": "latest",
    "sass": "latest",
    "typescript": "latest",
    "unocss": "0.43.1",
    "unplugin-auto-import": "0.11.2",
    "vite": "latest",
    "vite-plugin-dts": "latest",
    "vite-plugin-mock": "latest",
    "vitepress": "latest",
    "vue": "latest",
    "vue-router": "latest",
    "vuetify": "next"
  },
  "lint-staged": {
    "{src,__tests__}/**.*": [
      "eslint --fix"
    ]
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  }
}
