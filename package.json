  "peerDependenciesMeta": {
    "@vue/composition-api": {
      "optional": true
    }
  },
  "dependencies": {
    "kayran": "latest",
    "lodash-es": "latest",
    "vue-demi": "latest",
    "vue-global-config": "latest"
  },
  "devDependencies": {
    "@antfu/eslint-config": "latest",
